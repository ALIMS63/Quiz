<div id="main">
  <p class="question">{{question}}</p>
  <div id="qestions">
    <form id="quizConditionForm">
      <ul id="answers">
        {{#each arrAnswers}}
        <li class="answers"><input type="checkbox" name={{this}} value={{this}} class="checkBox"> {{this}}</li>
        {{/each}}
      </ul>
      <input id="submitToCondition" type="submit" value="Отправить" class="btn btn-primary btn-sm">
    </form>
    <input id="backToReality" type="submit" value="Назад">
  </div>
</div>